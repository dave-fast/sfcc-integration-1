  "scripts": {
    "build": "webpack --mode=production --env CHECKOUT_URL='https://checkout.dev.slow.dev' --env API_URL='https://api.dev.slow.dev'",
    "deploy": "true",
    "lint": "eslint --ext .ts,.tsx \"$(pwd)/**/**/*.{ts,tsx}\" && tsc",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "pretty:lint": "prettier-eslint \"$(pwd)/**/*.{js,jsx,ts,tsx}\" --write",
    "pretty:check": "prettier \"$(pwd)/**/*.{js,jsx,ts,tsx}\" --check --ignore-path ../.prettierignore",
    "serve": "serve .",
    "test": "CHECKOUT_URL=https://example.local jest --env=jest-environment-jsdom-sixteen",
    "test:coverage": "CHECKOUT_URL=https://example.local jest --coverage  --coverageReporters=json --coverageReporters=lcov --coverageReporters=json-summary --env=jest-environment-jsdom-sixteen",
    "test:watch": "CHECKOUT_URL=https://example.local jest --watch  --env=jest-environment-jsdom-sixteen"
  }
